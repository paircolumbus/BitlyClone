<p class="notice error"><%= flash[:error] %></p>
<h1>Listing urls</h1>

<table>
  <thead>
    <tr>
      <th>Long url</th>
      <th>Short url</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urls.each do |url| %>
      <tr>
        <td><%= url.long_url %></td>
        <td><a href="<%= short_url url %>"><%= short_url url %></a></td>
        <td><%= link_to 'Show', url %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Url', new_url_path %>
