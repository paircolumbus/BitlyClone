<h1>List of Shortened URLs</h1>

<table class="table table-striped table-condensed">
  <thead>
    <tr>
      <th>ID</th>
      <th>URL</th>
      <th>Shortened URL</th>
      <th>User</th>
      <th>Count</th>
      <th>Created</th>
      <th>Updated</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @urls.each do |urls| %>
      <tr>
        <td><%= urls.id %></td>
        <td><%= urls.url %></td>
        <td><%= urls.shortened_url %></td>
        <td><%= urls.user %></td>
        <td><%= urls.count %></td>
        <td><%= urls.created_at.to_formatted_s(:long) %></td>
        <td><%= urls.updated_at.to_formatted_s(:long) %></td>
      </tr>
    <% end %>
  </tbody>
</table>